.homeBottom{
  min-height: 250px;
  width: 100%;
  background: #FFFFFF;
  margin-top: 20px;
  box-sizing: border-box;
  /* overflow: hidden; */
  position: relative;
}
.homeBottom .homeBottomContent{
  width: 92%;
  /* height: 210px; */
  padding: 20px 4%;
}
.homeBottom .homeBottomContent image{
  width: 100%;
  height: 150px;
  border: 0;
}
.homeBottom .homeBottomContent .homeBottomTitle{
  width: 100%;
  min-height: 21px;
  line-height: 21px;
