/* pages/me/me.wxss */
.meBox {
  width: 100%;
  box-sizing: border-box;
}
.meBox .userinfo{
  width: 100%;
  height: 150px;
  position: relative;
  background: #fff;
  border-radius:4 
  
}

.meBox .userinfo image{
  width: 60px;
  height: 60px;
  border-radius: 50%;
  margin: 0 auto;  
  display: flex;
  justify-content: center;
 
}
.userinfo-nickname{
  display: block;
  width: 100%;
  text-align: center;
  margin-top:20px; 
  font-size:14px;
  font-family:PingFangSC-Regular;
  font-weight:400;
  color:rgba(5,5,9,1);
}
.meList{
  width: 100%;
  margin-top: 14px;
}
.meList view{
  width: 100%;
  height: 50px;
  background: #fff;
  padding: 0 10px;
  line-height: 50px;
  position: relative;
   font-size:14px;
  font-family:PingFangSC-Regular;
  font-weight:400;
  border-bottom: 1px solid #DFDFDF;
  box-shadow:0px 1px 0px 0px rgba(223,223,223,1);
}
.meList view image{
  width: 26px;
  height: 37px;
  position: absolute;
  right: 20px;
  top: 7px;
  cursor: pointer;
}
.outBnt{
  width: 100%;
  height: 50px;
  text-align: center;
  line-height: 50PX;
  margin-top: 10PX;
   font-size:14px;
  font-family:PingFangSC-Regular;
  font-weight:400;
  border-bottom: 1px solid #DFDFDF;
   border-top: 1px solid #DFDFDF;
   background: #fff;
}