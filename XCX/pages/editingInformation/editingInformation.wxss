/* pages/find/find.wxss */
#editingInformationBox{
  width: 100%;
}
#editingInformationBox .header{
  margin: 10px 0;
  width: 100%;
 
}
.editingInformationBoxItem{
  width: 96%;
  padding: 0 2%;
  height: 60px;
  background: #fff;
  background:rgba(255,255,255,1);
  box-shadow:0px -1px 0px 0px rgba(223,223,223,1);
  border-bottom:1px solid #D7D7D7; 
  line-height: 60px;
  font-size:14px;
  font-family:PingFangSC-Regular;
  font-weight:400;
  color:rgba(102,102,102,1);
  position: relative;
}
.editingInformationBoxItem1{
    width: 96%;
  padding: 0 2%;
  height: 50px;
  background: #fff;
  background:rgba(255,255,255,1);
  box-shadow:0px -1px 0px 0px rgba(223,223,223,1);
  border-bottom:1px solid #D7D7D7; 
  line-height: 50px;
  font-size:14px;
  font-family:PingFangSC-Regular;
  font-weight:400;
  color:rgba(102,102,102,1);
  position: relative;
}
.editingInformationBoxItem11{
    width: 96%;
  padding: 0 2%;
  height: 50px;
  background: #fff;
  background:rgba(255,255,255,1);
  box-shadow:0px -1px 0px 0px rgba(223,223,223,1);
  border-bottom:1px solid #D7D7D7; 
  line-height: 50px;
  font-size:14px;
  font-family:PingFangSC-Regular;
  font-weight:400;
  color:rgba(102,102,102,1);
  position: relative;
  display: flex;
}
.editingInformationBoxItemValue1{
  width: 56px;
  margin-right: 5px;
}
.editingInformationBoxItemValue{
  display: inline-block;
  flex: 1;
  white-space: nowrap;
  overflow: hidden;
  text-overflow: ellipsis;
}
.headPortrait{
  width: 40px;
  height: 40px;
  display: inline-block;
  float: right;
  margin-right: 20px;
   margin-top: 10px;
}
.headPortrait image{
  display: inline-block;
  width: 100%;
  height: 100%;
  border-radius: 50%;
}
.editingInformationBoxRight{
  display: inline-block;
  float: right;
  margin-right: 20px;
}
.editingInformationBoxImage{
  display: inline-block;
  width: 13px;
  height: 19px;
  position: absolute;
  top: 15px;
  right: 10px;
}
.editingInformationBoxOK{
  width: 100%;
  height: 50px;
  line-height: 50px;
  font-size:14px;
  font-family:PingFangSC-Regular;
  font-weight:400;
  color:rgba(102,102,102,1);
  background: #fff;
  text-align: center;
  margin-top: 15px;
}
.commonCententinput{
  height: 49px;
  line-height: 49px;
  font-size:14px;
  font-family:PingFangSC-Regular;
  font-weight:400;
  color:rgba(5,5,9,1);
  border: 1px solid #eeeeee;
  border-radius: 4px;
}
.commonCententTextarea{
  min-height: 100px;
  width: 100%;
  font-size:14px;
  font-family:PingFangSC-Regular;
  font-weight:400;
  color:rgba(5,5,9,1);
   border: 1px solid #eeeeee;
  border-radius: 4px;
}